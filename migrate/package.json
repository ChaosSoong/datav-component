{
  "name": "@heating-power/migrate",
  "version": "0.0.2",
  "dependencies": {
    "bcore": "0.0.18",
    "echarts": "4.1.0",
    "jquery": "2.1.4",
    "lodash": "4.17.11"
  },
  "datav": {
    "cn_name": "迁徙图",
    "icon": "",
    "protocol": 2,
    "type": [
      "regular"
    ],
    "view": {
      "width": 800,
      "height": 600,
      "minWidth": 200,
      "minHeight": 100
    },
    "apis": {
      "source": {
        "handler": "render",
        "description": "接口描述",
        "fields": {
          "value": {
            "description": "值说明"
          }
        }
      }
    },
    "config": {
      "size": {
        "name": "字号",
        "type": "number",
        "default": 22,
        "range": [
          10,
          100
        ]
      },
      "backgroundColor": {
        "name": "背景颜色",
        "type": "color",
        "default": "rgba(64,74,89,0)"
      },
      "GeoitemStyleAreaColor": {
        "name": "地图颜色",
        "type": "color",
        "default": "#3f5461"
      },
      "seriesItemStyle": {
        "type": "group",
        "name": "闪烁区域",
        "children": {
          "color": {
            "type": "color",
            "name": "颜色",
            "default": "rgba(15,124,127,0.74)"
          },
          "radius": {
            "name": "字号",
            "type": "number",
            "default": 28,
            "range": [
              10,
              100
            ],
            "step": 1
          },
          "x": {
            "name": "经度",
            "type": "number",
            "default": 120.4651,
            "step": 0.001
          },
          "y": {
            "name": "纬度",
            "type": "number",
            "default": 36.3373,
            "step": 0.001
          }
        }
      },
      "lineItemStyle": {
        "type": "group",
        "name": "线条样式",
        "children": {
          "color": {
            "type": "color",
            "name": "颜色",
            "default": "rgba(13,229,235,0.74)"
          },
          "width": {
            "name": "线条宽度",
            "type": "number",
            "default": 0,
            "range": [
              0,
              100
            ],
            "step": 0.5
          },
          "curveness": {
            "name": "边的曲度",
            "type": "number",
            "default": 0.2,
            "range": [
              0,
              1
            ],
            "step": 0.1
          },
          "label": {
            "name": "是否显示标签",
            "type": "boolean",
            "default": false
          }
        }
      },
      "PointerItemStyle": {
        "type": "group",
        "name": "地理坐标点样式",
        "children": {
          "color": {
            "type": "color",
            "name": "颜色",
            "default": "rgba(143,118,240,0.74)"
          },
          "size": {
            "name": "半径",
            "type": "number",
            "default": 4,
            "range": [
              0,
              100
            ],
            "step": 1
          },
          "label": {
            "name": "是否显示标签",
            "type": "boolean",
            "default": false
          }
        }
      },
      "PlateItemStyle": {
        "type": "group",
        "name": "飞机样式",
        "children": {
          "color": {
            "type": "color",
            "name": "颜色",
            "default": "rgba(9,11,11,0.74)"
          },
          "size": {
            "name": "半径",
            "type": "number",
            "default": 10,
            "range": [
              0,
              100
            ],
            "step": 1
          },
          "show": {
            "name": "是否显示",
            "type": "boolean",
            "default": true
          }
        }
      }
    },
    "api_data": {
      "source": [
        {
          "value": "值"
        }
      ]
    }
  }
}
